{
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "ErrorDocument": {
        "Key": "404.html"
    },
    "RoutingRules": [
        {
          "Condition": {
            "HttpErrorCodeReturnedEquals": "404"
          },
          "Redirect": {
            "ReplaceKeyWith": "404.html"
          }
        }
    ]
}